#include <stdio.h>
int inser_arr(int arr[10],int l)
{
     int pos,ele;
   printf("Enter the pos and element to be inserted:");
   scanf("%d %d",&pos,&ele);
   for(int i=1;i<=l-pos;i++)
   {
       arr[l-i]=arr[l-1-i];
   }
    arr[pos-1]=ele;
    printf("Elements after insertion:");
    for(int i=0;i<l;i++)
    printf("%d ",arr[i]);
}
int main()
{
   int arr[10]={1,3,4,5,6,7,8,9,10};
   int l=sizeof(arr)/sizeof(int);
   int (*fptr)(int[],int);
   fptr=inser_arr;
   fptr(arr,l);
   return 0;
}
