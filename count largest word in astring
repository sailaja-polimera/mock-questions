#include <stdio.h>
int main()
{
   char s[]="I worked at thundersoft ";
   int count=0,temp=0;
     for(int i=0;s[i]!='\0';i++)
     {
       count++;
   if(s[i]==' ')
   {
      if (temp<count)
      {
          temp = count-1;
          count =0;
      }
      count =0;
     }
     }
    printf("%d ",temp);
}
